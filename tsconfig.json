{
    "compilerOptions": {
       "lib": [
          "es5",
          "es6",
          "DOM"
       ],
       "target": "es5",
       "module": "commonjs",
       "moduleResolution": "node",
       "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "*": ["node_modules/*", "src/types/*"]
        },
       "emitDecoratorMetadata": true,
       "experimentalDecorators": true,
       "sourceMap": true
    },
    "include": ["./src/**/*"]
  }